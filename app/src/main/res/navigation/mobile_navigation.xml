<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_inicio">

    <fragment
        android:id="@+id/nav_inicio"
        android:name="com.example.chatappwhere.ui.home.HomeFragment"
        android:label="Inicio"
        tools:layout="@layout/fragment_inicio" />

    <fragment
        android:id="@+id/nav_Calendar"
        android:name="com.example.chatappwhere.ui.gallery.GalleryFragment"
        android:label="Calendario"
        tools:layout="@layout/fragment_calendar"
        />

    <fragment
        android:id="@+id/nav_Maps"
        android:name="com.example.chatappwhere.ui.slideshow.SlideshowFragment"
        android:label="Mapa"
        tools:layout="@layout/fragment_map" />

    <fragment
        android:id="@+id/nav_messenger"
        android:name="com.example.chatappwhere.ui.tools.ToolsFragment"
        android:label="Mensajes"
        tools:layout="@layout/fragment_mensajes" />

    <fragment
        android:id="@+id/nav_nots"
        android:name="com.example.chatappwhere.ui.share.ShareFragment"
        android:label="Notas"
        tools:layout="@layout/fragment_notas" />

    <fragment
        android:id="@+id/nav_exit"
        android:name="com.example.chatappwhere.ui.send.SendFragment"
        android:label="Cerrar Sesion"
        tools:layout="@layout/fragment_exit" />
</navigation>